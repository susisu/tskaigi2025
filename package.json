  },
  "dependencies": {
    "@susisu/effectful": "^0.7.1"
  },
  "devDependencies": {
    "@susisu/eslint-config": "^0.0.97",
    "@types/node": "^22.15.21",
    "@vitest/coverage-v8": "^3.1.4",
    "eslint": "^9.27.0",
    "globals": "^16.1.0",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "typescript": "~5.8.3",
    "vitest": "^3.1.4"
  }
}
